{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": 2
    },
    "globals": {
        "__OGV_FULL_VERSION__": true,
        "ActiveXObject": true,
        "MSStreamReader": true,
        "Float32Array": true,
        "Uint8Array": true,
        "Uint32Array": true,
        "setImmediate": true,
        "Promise": true,
        "VideoDecoder": true,
        "ArrayBuffer": true
    }
}
